{
  "name": "vercel-demo",
  "version": "0.1.0",
  "packageManager": "pnpm@9.15.0",
  "private": true,
  "scripts": {
    "build": "pnpm turbo db:build app:build",
    "app:build": "next build",
    "db:build": "pnpm db:generate --no-engine",
    "db:client": "prisma studio",
    "db:create-migration": "prisma migrate dev --create-only",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:reset": "prisma migrate reset",
    "db:seed": "prisma db seed -- --environment development",
    "check-format": "prettier --check .",
    "check-lint": "next lint",
    "check-types": "tsc --noEmit",
    "check": "turbo check-format check-lint check-types",
    "components": "pnpm dlx shadcn@latest add",
    "dev": "next dev --turbopack",
    "fixup": "turbo format lint check-types",
    "format": "prettier --write .",
    "lint": "next lint --fix",
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky",
    "start": "next start"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.10.0",
    "@prisma/client": "^6.10.1",
    "@prisma/extension-accelerate": "^2.0.1",
    "@radix-ui/react-accordion": "^1.2.11",
    "@radix-ui/react-alert-dialog": "^1.1.14",
    "@radix-ui/react-aspect-ratio": "^1.1.7",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-collapsible": "^1.1.11",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-hover-card": "^1.1.14",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-menubar": "^1.1.15",
    "@radix-ui/react-navigation-menu": "^1.2.13",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.3.7",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tooltip": "^1.2.7",
    "bcrypt": "^6.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "commitizen": "^4.3.1",
    "date-fns": "^4.1.0",
    "dotenv": "^17.0.0",
    "embla-carousel-react": "^8.6.0",
    "input-otp": "^1.4.2",
    "lucide-react": "^0.523.0",
    "next": "15.4.0-canary.109",
    "next-auth": "5.0.0-beta.29",
    "next-themes": "^0.4.6",
    "react": "^19.0.0",
    "react-day-picker": "^9.7.0",
    "react-dom": "^19.0.0",
    "react-resizable-panels": "^3.0.3",
    "recharts": "^3.0.0",
    "tailwind-merge": "^3.3.1",
    "vaul": "^1.1.2",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@next/eslint-plugin-next": "^15.3.4",
    "@tailwindcss/postcss": "^4",
    "@types/bcrypt": "^5.0.2",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9",
    "eslint-config-next": "15.3.4",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "husky": "^9.1.7",
    "prettier": "3.6.0",
    "prisma": "^6.10.1",
    "tailwindcss": "^4",
    "tsx": "^4.20.3",
    "turbo": "^2.5.4",
    "tw-animate-css": "^1.3.4",
    "typescript": "^5"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "prisma": {
    "seed": "tsx lib/db/seed.ts"
  }
}
